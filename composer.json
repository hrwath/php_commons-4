{
    "name": "cepa/php_commons-4",
    "description": "PHP Commons Library",
    "keywords": ["Commons"],
    "type": "library",
    "homepage": "https://github.com/cepa/php_commons-4",
    "license": "BSD",
    "authors": [
        {
            "name": "Lukasz Cepowski",
            "homepage": "http://lukasz.cepowski.com"
        }
    ],
    "version": "3",
    "require": {
        "php": ">=5.3.0",
        "psr/log": "~1.0",
        "nrk/predis": "v0.8.3",
        "thobbs/phpcassa": "v1.0.a.6"
    },
    "autoload": {
        "psr-0": {
            "Commons": ["src/", "test/"],
            "Mock": "test/",
            "Predis": "vendor/nrk/predis/lib/"
        },
        "files": ["vendor/thobbs/phpcassa/lib/autoload.php"]
    },
    "repositories": {
        "nrk/predis": {
            "type": "package",
            "package": {
                "name": "nrk/predis",
                "version": "v0.8.3",
                "source": {
                    "url": "https://github.com/nrk/predis.git",
                    "type": "git",
                    "reference": "v0.8.3"
                }
            }
        },
        "thobbs/phpcassa": {
            "type": "package",
            "package": {
                "name": "thobbs/phpcassa",
                "version": "v1.0.a.6",
                "source": {
                    "url": "https://github.com/thobbs/phpcassa",
                    "type": "git",
                    "reference": "v1.0.a.6"
                }
            }
        }
    }
}
