{
    "name": "cepa/php_commons-4",
    "description": "PHP Commons Library",
    "keywords": ["Commons"],
    "type": "library",
    "homepage": "https://github.com/cepa/php_commons-4",
    "license": "BSD",
    "authors": [
        {
            "name": "PHP Commons Contributors",
            "homepage": "http://www.phpcommons.com"
        },
        {
            "name": "Lukasz Cepowski",
            "homepage": "http://lukasz.cepowski.com"
        }
    ],
    "require": {
        "cepa/Beautify": "dev-master",
        "easybook/geshi": "1.0.8.11",
        "michelf/php-smartypants": "dev-lib",
        "michelf/php-markdown": "1.3.*@dev",
        "mustache/mustache": "~2.1",
        "predis/predis": "v0.8.3",
        "php": ">=5.3.0",
        "psr/log": "~1.0",
        "thobbs/phpcassa": "v1.1.0"
    },
    "autoload": {
        "psr-0": {
            "Commons": ["src/", "test/"],
            "Mock": "test/",
            "Predis": "vendor/nrk/predis/lib/"
        },
        "files": [
            "./vendor/cepa/Beautify/beautify.php"
        ]
    },
    "repositories": {
        "cepa/Beautify": {
            "type": "package",
            "package": {
                "name": "cepa/Beautify",
                "version": "dev-master",
                "source": {
                    "url": "https://github.com/cepa/Beautify.git",
                    "type": "git",
                    "reference": "master"
                }
            }
        },
        "cepa/php-smartypants": {
            "type": "package",
            "package": {
                "name": "cepa/php-smartypants",
                "version": "dev-master",
                "source": {
                    "url": "https://github.com/cepa/php-smartypants.git",
                    "type": "git",
                    "reference": "master"
                }
            }
        }
    }
}
